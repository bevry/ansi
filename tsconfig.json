{
  "compilerOptions": {
    "allowJs": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "lib": ["ESNext"],
    "maxNodeModuleJsDepth": 5,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "target": "es2023"
  },
  "include": ["source"]
}
